<?xml version="1.0" encoding="utf-8" ?>
<objects xmlns="http://www.springframework.net">
  <object id="settersAndAbsquatulateAdvisor"
      type="Spring.Aop.Support.RegularExpressionMethodPointcutAdvisor, Spring.Aop">
    <property name="advice">
      <ref local="timerInterceptor"/>
    </property>
    <property name="patterns">
      <list>
        <value>.*.*</value>
      </list>
    </property>
  </object>

  <object id="timerInterceptor" type="MVC4Base.Interceptor.TimeCheckInterceptor">
  </object>
</objects>